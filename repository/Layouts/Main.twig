<!doctype html>
<html class="no-js" lang="en">
    <head>
        {{ this.partial('partials/head.twig')|raw }}
    </head>

    <!--[if lt IE 7]> <body class="lt-ie10 lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
    <!--[if IE 7]>    <body class="lt-ie10 lt-ie9 lt-ie8"> <![endif]-->
