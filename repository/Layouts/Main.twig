<!doctype html>
<html class="no-js" lang="en">
    <head>
        {{ this.partial('partials/head.twig')|raw }}
    </head>

    <!--[if lt IE 7]> <body class="lt-ie10 lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
    <!--[if IE 7]>    <body class="lt-ie10 lt-ie9 lt-ie8"> <![endif]-->
    <!--[if IE 8]>    <body class="lt-ie10 lt-ie9"> <![endif]-->
    <!--[if IE 9]>    <body class="lt-ie10"> <![endif]-->
    <!--[if gt IE 8 | !(IE)]><!-->
    <body>
    <!--<![endif]-->
        {{ this.bbtoolbar()|raw }}

        <div id="fb-root"></div>

        <script>
            !function(d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0],
                    p = /^http:/.test(d.location) ? 'http' : 'https';
                if (!d.getElementById(id)) {
                    js = d.createElement(s);
                    js.id = id;
                    js.src = p + "://platform.twitter.com/widgets.js";
                    fjs.parentNode.insertBefore(js, fjs);
                }
            }(document, "script", "twitter-wjs");
        </script>

        <div id="bb5-site-wrapper" class="{% block layout_css_class %}{% endblock %}">

            {{ this.partial('partials/header.twig')|raw }}

            <nav id="nav" class="nav navbar-wrapper">
                <div class="container">
                    {{ this.navbar('header')|raw }}
                </div>
            </nav>

            {% block beforeMainContent %}
                {{ this.breadcrumb()|raw }}
            {% endblock %}

            <div class="main container" role="main" itemprop="mainContentOfPage">
                {% block content %}{% endblock %}
            </div>

            {{ this.partial('partials/footer.twig')|raw }}
        </div>
    </body>
</html>